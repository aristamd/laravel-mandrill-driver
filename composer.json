{
    "name": "aristamd/laravel-mandrill-driver",
    "description": "Mandrill Driver for Laravel 6+",
    "type": "library",
    "keywords": ["Laravel","Mandrill"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Foster",
            "email": "mfoster@aristamd.com"
        },
        {
            "name": "Rob Fonseca",
            "email": "robfonseca@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.2",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelMandrill\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMandrill\\MandrillServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
